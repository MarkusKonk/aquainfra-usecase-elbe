{
    "version": "0.1.0",
    "id": "combine-eurostat-data",
    "use_case": "Elbe",
    "title": {
        "en": "Attach human population statistics to EU NUTS3 regions."
    },
    "description": {
        "en": "Attach latest EU human population statistics to EU NUTS3 regions using NUTS3 version of own choice."
    },
    "jobControlOptions": ["sync-execute", "async-execute"],
    "keywords": ["elbe use case", "AquaINFRA", "keyword 3", "keyword 4"],
    "links": [{
        "type": "text/html",
        "rel": "about",
        "title": "information",
        "href": "https://example.org/process",
        "hreflang": "en-US"
    }],
    "inputs": {
        "country_code": {
            "title": "Country code",
            "description": "Official country code as short text string for country in focus.",
            "schema": {
                "type": "string",
                "enum": [
                    "DE",
                    "CZ"
                ]
            },
            "minOccurs": 1,
            "maxOccurs": 1,
            "keywords": []
        },
        "nuts_year": {
            "title": "NUTS3 version year",
            "description": "Year for EU NUTS3 region version to be used and combined with the latest EU annual human population statistics available.",
            "schema": {
                "type": "string",
                "enum": [
                    "2003",
                    "2006",
                    "2010",
                    "2013",
                    "2016",
                    "2021",
                    "2024"
                ]
            },
            "minOccurs": 1,
            "maxOccurs": 1,
            "keywords": []
        },
        "pop_year": {
            "title": "pop_year TITLE",
            "description": "pop_year DESCRIPTION",
            "schema": {
                "type": "string"
            },
            "minOccurs": 1,
            "maxOccurs": 1,
            "keywords": []
        }
    },
    "outputs": {
        "nuts3_pop_data": {
            "title": "NUTS3 geopackage with latest EU annual human population",
            "description": "NUTS3 geopackage for the inputted NUTS3 version with latest EU annual human population attached as attribute data",
            "schema": {
                "contentMediaType": "application/geopackage+sqlite3"
            }
        }
    },
    "example": {
        "inputs": {
            "country_code": "DE",
            "year": "2021"
        }
    }
}
